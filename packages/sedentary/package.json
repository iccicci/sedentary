  },
  "funding": {
    "url": "https://blockchain.info/address/1Md9WFAHrXTb3yPBwQWmUfv2RmzrtbHioB"
  },
  "homepage": "https://github.com/iccicci/sedentary/packages/sedentary#readme",
  "keywords": [
    "DB",
