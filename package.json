{
	"name": "sedentary",
	"version": "0.0.0",
	"description": "The ORM which never needs to migrate",
	"scripts": {
		"clean": "node -r ts-node/register utils.ts clean",
		"coverage": "npm run clean && nyc -r lcov -r text -r text-summary -r html mocha -r ts-node/register test/*ts",
		"ignore": "node -r ts-node/register utils.ts ignore",
		"prepare": "npm run ignore && tsc --declaration index.ts",
		"test": "npm run clean && mocha -r ts-node/register test/*ts"
	},
	"bugs": "https://github.com/iccicci/sedentary/issues",
	"repository": "https://github.com/iccicci/sedentary",
	"keywords": [
		"migration",
		"database",
		"DB",
		"ORM",
		"postgresql",
		"sqlite"
	],
	"engines": {
		"node": ">=10.0"
	},
	"author": "Daniele Ricci <daniele.icc@gmail.com> (https://github.com/iccicci)",
	"license": "MIT",
	"readmeFilename": "README.md",
	"types": "index.d.ts",
	"dependencies": {},
	"devDependencies": {
		"@types/mocha": "7.0.2",
		"@types/node": "13.11.1",
		"@typescript-eslint/eslint-plugin": "2.28.0",
		"@typescript-eslint/parser": "2.28.0",
		"eslint": "6.8.0",
		"mocha": "7.1.1",
		"nyc": "15.0.1",
		"ts-node": "8.8.2",
		"typescript": "3.8.3"
	}
}
