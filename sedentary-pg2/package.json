{
  "name": "sedentary-pg",
  "version": "0.0.0",
  "description": "The ORM which never needs to migrate - PostgreSQL",
  "scripts": {
    "clean": "node -r ts-node/register utils.ts clean",
    "coverage": "npm run clean && nyc -r lcov -r text -r text-summary -r html mocha -r ts-node/register test/*js test/*ts",
    "ignore": "node -r ts-node/register utils.ts ignore",
    "prepare": "npm run ignore && tsc --declaration",
    "test": "npm run clean && mocha -r ts-node/register test/*js test/*ts"
  },
  "bugs": "https://github.com/iccicci/sedentary-pg/issues",
  "repository": "https://github.com/iccicci/sedentary-pg",
  "homepage": "https://github.com/iccicci/sedentary-pg#readme",
  "keywords": [
    "migration",
    "database",
    "DB",
    "ORM",
    "postgresql",
    "sqlite"
  ],
  "engines": {
    "node": ">=10.0"
  },
  "author": "Daniele Ricci <daniele.icc@gmail.com> (https://github.com/iccicci)",
  "license": "MIT",
  "readmeFilename": "README.md",
  "types": "index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "8.0.3",
    "@types/node": "14.11.2",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "eslint": "7.9.0",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "printWidth": 200,
    "trailingComma": "none",
    "useTabs": false
  }
}
